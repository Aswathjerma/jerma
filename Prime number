public class Main{
    public static void main(String[] args){
        int n = 8;
        boolean prime = true;
        for(int i =2; i<n; i++ ){
            if(n%2 == 0){
                prime = false;
                break;
            }
        }
    if(prime == true){
        System.out.println("The number is prime number");
    }
    else{
        System.out.println("The number is not a prime number");
    }
    }
}
